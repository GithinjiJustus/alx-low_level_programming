#!/bin/bash
#Check if the CFILE variable is set
if [ -z "$CFILE" ]; then
 echo "CFILE variable is not set.Please exort the C file path to CFILE."
 exit 1
 fi

#Generate Intel Sytax assembly code 
gcc S -masm=intel "$CFILE" -0 "${CFILE%.*}.s"
echo "Intel Syntax assebly code generated and saved as ${CFILE%.*}.s"
